    "sass-loader": "^8.0.2",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "npm run clean-root && npm run clean-pkgs",
    "clean-dist": "lerna exec --parallel -- rm -rf ./dist",
    "clean-pkgs": "lerna clean -y && lerna exec --parallel -- rm -rf ./dist ./package-lock.json",
    "clean-root": "rm -rf ./node_modules ./package-lock.json",
    "force-publish": "lerna publish --force-publish",
    "postinstall": "lerna bootstrap",
    "storybook": "start-storybook -p 9001",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
